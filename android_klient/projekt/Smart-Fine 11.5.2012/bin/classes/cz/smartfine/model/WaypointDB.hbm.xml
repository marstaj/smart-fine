<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cz.smartfine.model.WaypointDB" table="geolocation">
    <id name="id" type="integer">
      <column name="ID"/>
      <generator class="identity"/>
    </id>
    <property name="badgeNumber" type="integer">
      <column name="BADGENUMBER" not-null="true"/>
    </property>
    <property name="longtitude" type="double">
      <column name="LONGTITUDE" not-null="true"/>
    </property>
    <property name="latitude" type="double">
      <column name="LATITUDE" not-null="true"/>
    </property>
    <property name="time" type="timestamp">
      <column name="TIME" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
