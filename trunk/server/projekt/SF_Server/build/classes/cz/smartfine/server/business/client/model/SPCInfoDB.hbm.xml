<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cz.smartfine.server.business.client.model.SPCInfoDB" table="spcblacklist">
    <id name="id" type="integer">
      <column name="ID"/>
      <generator class="identity"/>
    </id>
    <property name="spcNumber" type="string">
      <column length="10" name="SPCNUMBER" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
