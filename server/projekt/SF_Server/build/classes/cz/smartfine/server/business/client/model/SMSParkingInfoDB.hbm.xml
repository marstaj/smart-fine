<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cz.smartfine.server.business.client.model.SMSParkingInfoDB" table="smsparking">
    <id name="id" type="integer">
      <column name="ID"/>
      <generator class="identity"/>
    </id>
    <property name="parkingSince" type="timestamp">
      <column name="SINCE" not-null="true"/>
    </property>
    <property name="parkingUntil" type="timestamp">
      <column name="UNTIL" not-null="true"/>
    </property>
    <property name="vehicleRegistrationPlate" type="string">
      <column length="10" name="VRP" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
